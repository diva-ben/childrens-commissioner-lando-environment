{
    "name": "diva-dev/wp-lpt-environment",
    "description": "WP LPT Environment",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*"
            ]
        },
        {
            "type": "package",
            "package": {
                "name": "advanced-custom-fields/advanced-custom-fields-pro",
                "version": "5.9.5",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=b3JkZXJfaWQ9NDUwMTN8dHlwZT1kZXZlbG9wZXJ8ZGF0ZT0yMDE0LTExLTI1IDE3OjExOjA4"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "theseoframework/the-seo-framework-extension-manager",
                "version": "2.5.2",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://dl.theseoframework.com/get/plugin/latest/"
                }
            }
        },
        {
            "type": "vcs",
            "url": "git@github.com:diva-dev/diva-staging-plugin.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:diva-dev/diva-skip-links.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:diva-dev/formidable-pro.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:diva-dev/formidable-mailchimp.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:diva-dev/formidable-export-view.git"
        }
    ],
    "require": {
        "aws/aws-sdk-php":"*",
        "advanced-custom-fields/advanced-custom-fields-pro": "*",
        "wpackagist-plugin/autodescription": "^4.2",
        "wpackagist-plugin/classic-editor": "*",
        "wpackagist-plugin/custom-post-type-ui": "*",
        "wpackagist-plugin/easy-wp-smtp": "*",
        "wpackagist-plugin/edit-author-slug": "*",
        "wpackagist-plugin/formidable": "*",
        "wpackagist-plugin/popup-maker": "*",
        "wpackagist-plugin/query-monitor": "*",
        "wpackagist-plugin/redirection": "*",
        "wpackagist-plugin/redis-cache": "*",
        "wpackagist-plugin/slim-maintenance-mode": "*",
        "wpackagist-plugin/wp-extra-file-types": "*",
        "diva-dev/diva-staging-plugin": "*",
        "diva-dev/diva-skip-links": "*",
        "diva-dev/formidable-pro": "*",
        "diva-dev/formidable-mailchimp": "*",
        "diva-dev/formidable-export-view": "*",
        "theseoframework/the-seo-framework-extension-manager": "*"
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/"
        }
    },
    "extra": {
        "installer-paths": {
            "wordpress/wp-content/mu-plugins/{$name}/": [
            ],
            "wordpress/wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}
